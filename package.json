{
  "name": "geojson-rewind",
  "version": "0.2.0",
  "description": "enforce winding order for geojson",
  "main": "index.js",
  "bin": "geojson-rewind",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap test/rewind.js --coverage",
    "build": "browserify -s geojsonRewind index.js > geojson-rewind.js && uglifyjs geojson-rewind.js -c > geojson-rewind.min.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/geojson-rewind.git"
  },
  "keywords": [
    "geojson",
    "winding",
    "order",
    "rendering",
    "coordinates"
  ],
  "author": "Tom MacWright",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mapbox/geojson-rewind/issues"
  },
  "homepage": "https://github.com/mapbox/geojson-rewind",
  "dependencies": {
    "@mapbox/geojson-area": "0.2.1",
    "minimist": "1.2.0",
    "concat-stream": "~1.5.2"
  },
  "devDependencies": {
    "browserify": "^14.1.0",
    "cz-conventional-changelog": "^1.2.0",
    "tap": "~8.0.1",
    "uglify-js": "^2.8.15"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
